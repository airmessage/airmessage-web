{
  "name": "airmessage-react",
  "version": "1.1.2",
  "author": "Tagavari",
  "description": "iMessage for the web, Windows, and Linux",
  "private": true,
  "sideEffects": [
    "**/*.css",
    "./{browser,electron-renderer}/init.ts"
  ],
  "scripts": {
    "start": "webpack serve --open",
    "start-secure": "webpack serve --open --env secure",
    "build": "webpack build",
    "electron-launch": "cross-env ELECTRON_START_URL=http://localhost:8080 electron electron-main/electron.js",
    "electron-start": "concurrently \"webpack serve --env electron\" \"wait-on tcp:8080 && npm run electron-launch\"",
    "electron-build": "webpack build --env electron",
    "electron-package": "npm run electron-build && electron-builder"
  },
  "devDependencies": {
    "@electron/typescript-definitions": "^8.8.0",
    "@types/bytebuffer": "^5.0.42",
    "@types/gapi": "^0.0.39",
    "@types/gapi.auth2": "^0.0.54",
    "@types/gapi.people": "^1.0.5",
    "@types/luxon": "^1.26.3",
    "@types/markdown-to-jsx": "^6.11.3",
    "@types/pako": "^1.0.1",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/spark-md5": "^3.0.2",
    "@types/ua-parser-js": "^0.7.35",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "awesome-node-loader": "^1.1.1",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.2",
    "electron": "^12.0.0",
    "electron-builder": "^22.9.1",
    "eslint": "^7.20.0",
    "eslint-plugin-react": "^7.22.0",
    "fork-ts-checker-webpack-plugin": "^6.1.1",
    "native-ext-loader": "^2.3.0",
    "node-addon-loader": "^0.0.6",
    "node-loader": "^1.0.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.18",
    "typescript": "^4.1.5",
    "wait-on": "^5.2.1",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "workbox-webpack-plugin": "^6.1.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@sentry/react": "^6.2.5",
    "bytebuffer": "^5.0.1",
    "electron-context-menu": "^2.4.0",
    "firebase": "^8.3.2",
    "libphonenumber-js": "^1.9.15",
    "luxon": "^1.25.0",
    "markdown-to-jsx": "^7.1.1",
    "pako": "^2.0.3",
    "react": "^17.0.1",
    "react-anchorme": "^2.1.1",
    "react-dom": "^17.0.1",
    "react-flip-toolkit": "^7.0.13",
    "spark-md5": "^3.0.1",
    "ua-parser-js": "^0.7.27",
    "uuid": "^8.3.2"
  },
  "optionalDependencies": {
    "airmessage-winrt": "file:native/airmessage-winrt"
  }
}
